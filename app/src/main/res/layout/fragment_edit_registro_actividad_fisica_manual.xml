            app:layout_constraintBottom_toTopOf="@+id/guideline180"
            app:layout_constraintEnd_toStartOf="@+id/guideline183"
            app:layout_constraintHorizontal_bias="0.081"
            app:layout_constraintStart_toStartOf="@+id/guideline181"
            app:layout_constraintTop_toTopOf="@+id/guideline179" />

        <EditText
            android:id="@+id/kcal_quemadas_input_edit"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:autoText="true"
            android:ems="10"
            android:gravity="center"
            android:hint="0"
            android:inputType="number|numberDecimal"
            android:numeric="decimal"
            android:textIsSelectable="false"
            app:layout_constraintBottom_toTopOf="@+id/guideline187"
            app:layout_constraintEnd_toStartOf="@+id/guideline183"
            app:layout_constraintHorizontal_bias="0.065"
            app:layout_constraintStart_toStartOf="@+id/guideline181"
            app:layout_constraintTop_toTopOf="@+id/guideline166"
            app:layout_constraintVertical_bias="0.591" />

