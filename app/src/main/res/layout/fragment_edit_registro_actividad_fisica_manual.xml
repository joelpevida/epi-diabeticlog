            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/guideline180"
            app:layout_constraintEnd_toStartOf="@+id/guideline182"
            app:layout_constraintHorizontal_bias="0.934"
            app:layout_constraintStart_toStartOf="@+id/guideline184"
            app:layout_constraintTop_toTopOf="@+id/guideline179"
            app:layout_constraintVertical_bias="0.485" />

        <TextView
            android:id="@+id/hora_final_edit"
            android:layout_width="0dp"
