<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ActividadFisicaManual.Model.RegistroActividadFisicaManual">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/textiew1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="@string/Intensidad"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/guideline176"
            app:layout_constraintEnd_toStartOf="@+id/guideline183"
            app:layout_constraintHorizontal_bias="0.081"
            app:layout_constraintStart_toStartOf="@+id/guideline181"
            app:layout_constraintTop_toTopOf="@+id/guideline175"
            app:layout_constraintVertical_bias="0.297" />

        <TextView
            android:id="@+id/textiew2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="@string/HoraInicio"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/guideline176"
            app:layout_constraintEnd_toStartOf="@+id/guideline182"
            app:layout_constraintHorizontal_bias="0.933"
            app:layout_constraintStart_toStartOf="@+id/guideline184"
            app:layout_constraintTop_toTopOf="@+id/guideline175"
            app:layout_constraintVertical_bias="0.297" />

        <TextView
            android:id="@+id/hora_inicio_edit"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:clickable="true"
            android:gravity="center"
            android:hint="00:00"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/guideline178"
            app:layout_constraintEnd_toStartOf="@+id/guideline182"
            app:layout_constraintHorizontal_bias="0.937"
            app:layout_constraintStart_toStartOf="@+id/guideline184"
            app:layout_constraintTop_toTopOf="@+id/guideline177"
            app:layout_constraintVertical_bias="0.369" />

        <TextView
