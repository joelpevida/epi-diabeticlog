<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Epidiabeticlog" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/azul_fondo</item>
        <item name="colorPrimaryVariant">@color/azul_fondo</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/azul_fondo</item>
        <item name="colorSecondaryVariant">@color/azul_fondo</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color.  <item name="colorOnSecondary">@color/black</item>        -->
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Epidiabeticlog.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Epidiabeticlog.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Epidiabeticlog.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>